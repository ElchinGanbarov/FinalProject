{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery-toast-plugin@1.3.2",
      "destination": "wwwroot/lib/jquery-toast-plugin/"
    },
    {
      "library": "limonte-sweetalert2@8.11.8",
      "destination": "wwwroot/lib/limonte-sweetalert2/",
      "files": [
        "sweetalert2.min.css",
        "sweetalert2.all.min.js"
      ]
    },
    {
      "library": "jquery-confirm@3.3.4",
      "destination": "wwwroot/lib/jquery-confirm/"
    },
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/lib/jqueryui/",
      "files": [
        "jquery-ui.min.js",
        "themes/base/jquery-ui.css",
        "jquery-ui.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.6",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js.map",
        "dist/browser/signalr.js.map"
      ]
    }
  ]
}